// JavaScript Document
		$(function () {
            $(".admin-area a.btn_right").click(function(){
				$(".admin-area ul").css("height","auto" );
				$(".admin-area a.btn_right").css("display","none");
				$(".admin-area a.btn_right_shut").css("display","inline");
			});
            $(".admin-area a.btn_right_shut").click(function(){
				$(".admin-area ul").css("height","30px" );
				$(".admin-area a.btn_right_shut").css("display","none");
				$(".admin-area a.btn_right").css("display","inline");
			});			

            $(".logistics-circle a.btn_right").click(function(){
				$(".logistics-circle ul").css("height","auto" );
				$(".logistics-circle a.btn_right").css("display","none");
				$(".logistics-circle a.btn_right_shut").css("display","inline");
			});
            $(".logistics-circle a.btn_right_shut").click(function(){
				$(".logistics-circle ul").css("height","30px" );
				$(".logistics-circle a.btn_right_shut").css("display","none");
				$(".logistics-circle a.btn_right").css("display","inline");
			});								
				});