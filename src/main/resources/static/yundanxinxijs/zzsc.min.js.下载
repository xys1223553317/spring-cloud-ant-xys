$(function(){var a={hoverqq:function(a){a?($(".yb_conct").css("right","5px"),$(".yb_bar .yb_ercode").css("height","200px")):($(".yb_conct").css("right","-127px"),$(".yb_bar .yb_ercode").css("height","53px"))},gototop:function(a){$("body,html").animate({scrollTop:0},a)}},b=function(){$(".yb_conct").hover(function(){a.hoverqq(1)},function(){a.hoverqq(0)}),$(".yb_top").click(function(){a.gototop(300)})};b()});